{

    "manifest_version": 2,
    "name": "Twitch Pause",
    "version": "1.1",

    "description": "Twitch pause extension for Mozilla",

    "icons": {
        "48": "icons/icon.png"
    },

    "content_scripts": [
        {
            "matches": ["*://www.twitch.tv/*"],
            "js": ["script.js"]
        }
    ]

}

